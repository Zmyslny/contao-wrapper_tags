<?php if ($this->tags): ?>
    <table class="wt <?= $this->version ?>">
        <tbody>
        <?php foreach ($this->tags as $tag): ?>
            <tr>
                <td>
                    &lt;/<?php echo $tag['tag'] ?>&gt;
                </td>
            </tr>
        <?php endforeach ?>
        </tbody>
    </table>
<?php else: ?>
    <span class="tl_red"><?php echo $GLOBALS['TL_LANG']['MSC']['wt.dataCorrupted'] ?></span>
<?php endif ?>

<?php if ($this->wildcard): ?>
    <div class="tl_gray">
        <?php echo $this->wildcard ?>
    </div>
<?php endif ?>
